#!/usr/bin/env bash
export RABBITMQ_IMAGE=${RABBITMQ_IMAGE:="rabbitmq"}
export RABBITMQ_IMAGE_VERSION=${RABBITMQ_IMAGE_VERSION:="3.5.4-management"}
export RABBITMQ_ROOT=/var/lib/dokku/services/rabbitmq

export PLUGIN_COMMAND_PREFIX="rabbitmq"
export PLUGIN_DATA_ROOT=${PLUGIN_DATA_ROOT:="$RABBITMQ_ROOT"}
export PLUGIN_DATASTORE_PORTS=(5672 4369 35197 15672)
export PLUGIN_DEFAULT_ALIAS="RABBITMQ"
export PLUGIN_IMAGE=$RABBITMQ_IMAGE
export PLUGIN_IMAGE_VERSION=$RABBITMQ_IMAGE_VERSION
export PLUGIN_SCHEME="rabbitmq"
export PLUGIN_SERVICE="RabbitMQ"
